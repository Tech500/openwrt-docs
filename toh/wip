	<thead>
	<tr class="row0">
		<th class="col0"> Model </th><th class="col1"> Version </th><th class="col2"> Status </th><th class="col3"> Target(s) </th><th class="col4"> Platform </th><th class="col5"> CPU Speed (<abbr title="Megahertz">MHz</abbr>) </th><th class="col6"> Flash (<abbr title="Megabyte">MB</abbr>) </th><th class="col7"> RAM (<abbr title="Megabyte">MB</abbr>) </th><th class="col8"> Wireless NIC </th><th class="col9"> Wireless Standard </th><th class="col10"> Wired Ports </th><th class="col11"> VLAN Config </th><th class="col12"> USB </th><th class="col13"> JTAG </th>
	</tr>
	</thead>
