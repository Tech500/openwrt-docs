├── sbin
│   ├── ifconfig -&gt; ../bin/busybox
│   ├── init.d
│   │   ├── rc0.d
│   │   │   └── 00syslog -&gt; ../rc.syslog
│   │   ├── rc1.d
│   │   │   ├── 00syslog -&gt; ../rc.syslog
│   │   │   ├── 10mps -&gt; ../rc.mps
│   │   │   └── 11fcnf -&gt; ../rc.1stcnf
│   │   ├── rc.1stcnf
