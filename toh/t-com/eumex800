│   ├── firewall.lan
│   ├── firewall.reset
│   ├── group
│   ├── hosts
│   ├── inittab
│   ├── issue
│   ├── motd
│   ├── passwd
│   ├── rc.config
│   ├── resolv.conf
│   ├── route.conf
│   ├── services
│   ├── syslog.conf
│   └── wantab
├── lib
│   └── modules
│       ├── ab_scan
│       ├── clip
│       ├── codec
│       ├── ihl
│       ├── led_compactdsl
│       ├── mfv
│       ├── sswmux
│       └── usw
├── linux.bin.gz
├── proc
