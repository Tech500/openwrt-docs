│   │   │   ├── 20netdev -&gt; ../rc.netdev
│   │   │   ├── 21network -&gt; ../rc.network
│   │   │   ├── 22route -&gt; ../rc.route
│   │   │   ├── 30dhcpd -&gt; ../rc.dhcpd
│   │   │   ├── 30dnsd -&gt; ../rc.dnsd
│   │   │   ├── 31bccd -&gt; ../rc.bccd
│   │   │   ├── 31capid -&gt; ../rc.capid
│   │   │   ├── 31tapid -&gt; ../rc.tapid
│   │   │   └── 33statd -&gt; ../rc.statd
│   │   ├── rc3.d
│   │   │   ├── 00syslog -&gt; ../rc.syslog
│   │   │   ├── 10mps -&gt; ../rc.mps
│   │   │   ├── 12cnfd -&gt; ../rc.cnfd
