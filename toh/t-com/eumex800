│   │   │   ├── 31tapid -&gt; ../rc.tapid
│   │   │   ├── 33statd -&gt; ../rc.statd
│   │   │   └── 40ras -&gt; ../rc.ras
│   │   ├── rc4.d
│   │   │   ├── 00syslog -&gt; ../rc.syslog
│   │   │   ├── 10mps -&gt; ../rc.mps
│   │   │   ├── 12cnfd -&gt; ../rc.cnfd
│   │   │   ├── 20netdev -&gt; ../rc.netdev
