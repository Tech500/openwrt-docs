	<thead>
	<tr class="row0">
		<th class="col0"> CPU </th><th class="col1"> Ram </th><th class="col2"> Flash </th><th class="col3"> Network </th><th class="col4"> WLAN </th><th class="col5"> VoIP </th><th class="col6"> S0 </th><th class="col7"> USB </th><th class="col8"> Serial </th><th class="col9"> JTag </th>
	</tr>
	</thead>
	<tr class="row1">
